{
  "name": "wayfarer_api",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking server.",
  "main": "app.js",
  "scripts": {
    "tablesUp": "node -r esm ./src/db/createTables.db.js createAllTables",
    "tablesDown": "node -r esm ./src/db/createTables.db.js dropAllTables",
    "migrate": "node -r esm ./src/db/createTables.db.js dropAllTables && node -r esm ./src/db/createTables.db.js createAllTables && node -r esm ./src/db/migration.db.js seedTables",
    "build": "babel src --out-dir dist --copy-files",
    "clean": "rm -rf dist && mkdir dist",
    "cleanTest": "rm -rf coverage && mkdir coverage",
    "heroku-postbuild": "npm run migrate && npm run clean && npm run build",
    "lint": "eslint --ignore-path .eslintignore .",
    "test": "nyc mocha -r esm --cache false --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "node dist/app.js",
    "start:dev": "nodemon --exec babel-node src/app.js"
  },
  "nyc": {
    "exclude": [
      "src/app.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parkerthegeniuschild/WayFarer_API.git"
  },
  "keywords": [
    "booking",
    "server",
    "trip",
    "transport"
  ],
  "author": "MITCHELL PATRICK",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hapi/joi": "^15.1.0",
    "app-root-path": "^2.2.1",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "object-rename-keys": "^1.2.1",
    "path": "^0.12.7",
    "pg": "^7.11.0",
    "promise": "^8.0.3",
    "request": "^2.88.0",
    "runnable-exports": "^0.2.0",
    "superagent": "latest",
    "supports-color": "^7.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "ajv": "^6.10.1",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2"
  }
}
